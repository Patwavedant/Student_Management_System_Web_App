@model System.Data.DataTable

@{
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="row">
    <!-- left column -->
    <div class="col-md-12">
        <!-- general form elements -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Circular InActive</h3>
            </div>
            <div class="box-body">
                <table border="1" class="table table-bordered">
                    <tr>
                        <th>Title</th>
                        <th>CFile</th>
                        <th>PublishDate</th>
                        <th>Actions</th>
                    </tr>
                    @for (int i = 0; i < Model.Rows.Count; i++)
                    {
                        <tr>
                            <td>@Model.Rows[i]["Title"]</td>
                            <td>@Model.Rows[i]["CFile"]</td>
                            <td>@Model.Rows[i]["PublishDate"]</td>
                            <td><a href="/HOD/CircularEdit?ID=@Model.Rows[i]["CircularID"]">Edit</a></td>
                        </tr>
                    }
                </table>
            </div>
        </div>
        <!-- /.box -->
    </div>
    <!--/.col (left) -->
</div>



