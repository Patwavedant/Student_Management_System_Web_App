@using SMS.Models

@{    
        var Staff = Session["Staff"] as Staff;    

        Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
  <div class="row">
            <div class="col-md-4">                
                <!-- Profile Image -->
                <div class="box box-primary">                    
                    <div class="box-body box-profile">                        
                        <h3 class="profile-username text-center">User Profile</h3>
                         <ul class="list-group list-group-unbordered">
                            <li class="list-group-item">
                                <b>Name</b> <a class="pull-right">@Staff.SName</a>
                            </li>                            
                            <li class="list-group-item">
                                <b>Phone</b> <a class="pull-right">@Staff.Phone</a>
                            </li>
                            <li class="list-group-item">
                                <b>Email</b> <a class="pull-right">@Staff.Email</a>
                            </li>                            
                        </ul>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
            <!-- /.col -->
       </div>
   

